{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Location creation</h1>

    {{ form_start(form) }}
    {{ form_row(form.name) }}
    {{ form_row(form.description) }}
    {{ form_row(form.path) }}
    {{ form_row(form.url) }}
    {{ form_row(form.type) }}
    {{ form_rest(form) }}
    {{ form_end(form) }}

    <ul class="record_actions">
        <li>
            <a href="{{ path('location') }}">
                Back to the list
            </a>
        </li>
    </ul>

{% endblock %}
