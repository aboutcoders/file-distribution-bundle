<?xml version="1.0" encoding="UTF-8" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="abc_file_distribution.location_manager.class">Abc\FileDistributionBundle\Doctrine\LocationManager</parameter>
        <parameter key="abc_file_distribution.model.location.class">Abc\FileDistributionBundle\Entity\Location</parameter>
    </parameters>

    <services>
        <service id="abc_file_distribution.location_manager.default" class="%abc_file_distribution.location_manager.class%" public="false">
            <argument type="service" id="abc_file_distribution.entity_manager" />
            <argument>%abc_file_distribution.model.location.class%</argument>
        </service>
    </services>

</container>