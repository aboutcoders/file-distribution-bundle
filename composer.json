{
    "name": "aboutcoders/file-distribution-bundle",
    "type": "symfony-bundle",
    "description": "File distribution Symfony2 bundle",
    "keywords": ["file", "bundle", "distribution"],
    "homepage": "http://aboutcoders.com",
    "license": "AboutCoders",
    "authors": [
        {
            "name": "Wojciech Ciolko",
            "email": "w.ciolko@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "git@bitbucket.org:wciolko/file-distribution.git"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "phpunit/phpunit": "~4",
        "doctrine/orm": "~2.2,>=2.2.3",
        "doctrine/doctrine-bundle": "~1.2",
        "gedmo/doctrine-extensions": "dev-master",
        "doctrine/doctrine-fixtures-bundle": "dev-master",
        "aboutcoders/file-distribution": "dev-master"
    },
    "require-dev": {

    },
    "autoload": {
        "psr-0": {"Abc\\FileDistributionBundle": ""}
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "target-dir": "Abc/FileDistributionBundle"
}
